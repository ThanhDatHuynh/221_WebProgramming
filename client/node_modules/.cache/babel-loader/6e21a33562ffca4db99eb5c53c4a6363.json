AA,CAAA,IAAA,CAAA,QAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,YAAA,CAAA,GAAA,IAAA,CAAA,KAAA,CAAA,QAAA,EAAA,QAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,CAAA;AACA,OAJA;AAKA,KAdA;AAeA,IAAA,cAfA,4BAeA;AACA,UAAA,WAAA,GAAA,KAAA,IAAA;AACA,UAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,UAAA,CAAA,WAAA,CAAA,MAAA,CAAA,EAAA,WAAA,CAAA,MAAA,CAAA,GAAA,CAAA,CAAA,KACA,EAAA,WAAA,CAAA,MAAA,CAAA;AACA,WAAA,UAAA,CAAA,WAAA;AACA,WAAA,IAAA,GAAA,WAAA;AACA,WAAA,YAAA;AACA;AAvBA,GAnBA;AA6CA,EAAA,MAAA,EAAA,CAAA,MAAA,EAAA,YAAA,CA7CA;AA8CA,EAAA,WA9CA,yBA8CA;AACA,SAAA,aAAA;AACA;AAhDA,CAAA","sourcesContent":["\n<template>\n    <div>\n        <div class=\"title-wrapper\">\n            <div class=\"menu-title\">\n                <p>{{ this.data.name }}</p>\n            </div>\n        </div>\n        <div class=\"text-center\">\n            <img :src=\"this.data.image\" style=\"border-radius:10px;\" />\n        </div>\n        <div class=\"text-center my-5\" style=\"font-size:20px\">\n            <p>{{ this.data.description }}</p>\n        </div>\n        <div class=\"text-center my-5\" style=\"font-size:20px\">\n            <p>Price: {{ this.data.price }} $</p>\n        </div>\n        <div class=\"btn-wrapper d-flex flex-wrap justify-center\">\n            <Button @onClick=\"handleBtnClick\" text=\"Add To Cart\" width=\"148px\" height=\"53px\"  v-show=\"user != null\"/>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Button from \"../components/Button\";\n\nimport $ from \"jquery\";\n// import Dish from \"../components/Dish.vue\";\n// import Button from \"../components/Button.vue\";\nexport default {\n    name: \"MenuDetailPage\",\n    components: {\n        Button,\n    },\n    data() {\n        var managerValue = null;\n        var userValue = null;\n        if (localStorage.getItem(\"User\") != null) {\n            managerValue = JSON.parse(localStorage.getItem(\"User\")).manager;\n            userValue = localStorage.getItem(\"User\");\n        }\n        console.log(userValue);\n        return {\n            data: {},\n            user: userValue,\n            manager: managerValue,\n        };\n    },\n    methods: {\n        getMenuDetail() {\n            const __this = this;\n            var settings = {\n                url: `${process.env.VUE_APP_API_URL}/menu/{${this.$route.params.id}}`,\n                method: \"GET\",\n                timeout: 0,\n            };\n\n            $.ajax(settings)\n                .done(function (response) {\n                    const a = JSON.parse(response).response;\n                    __this.data = a;\n                })\n        },\n        handleBtnClick() {\n            let currentCart = this.cart;\n            let foodID = this.$route.params.id;\n            if (!currentCart[foodID]) currentCart[foodID] = 1;\n            else ++currentCart[foodID];\n            this.updateCart(currentCart);\n            this.cart = currentCart;\n            this.$forceUpdate();\n        },\n\n    },\n    inject: ['cart', 'updateCart'],\n    beforeMount() {\n        this.getMenuDetail();\n    },\n};\n</script>\n\n<style scoped>\n@import url(\"https://fonts.googleapis.com/css2?family=Oleo+Script+Swash+Caps&display=swap\");\n\n.title-wrapper {\n    margin-bottom: 4%;\n}\n\n.menu-title {\n    font-family: Arial, Helvetica, sans-serif;\n    text-align: center;\n    font-size: 400%;\n    margin: 2% 0% -1% 0%;\n    font-weight: bold;\n}\n\n.menu-description {\n    font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen,\n        Ubuntu, Cantarell, \"Open Sans\", \"Helvetica Neue\", sans-serif;\n    text-align: center;\n    margin: 0% 8% -1% 8%;\n}\n\n.btn-wrapper {\n    margin: 25px 0 65px 0;\n}\n</style>\n"],"sourceRoot":"src/pages"}]}