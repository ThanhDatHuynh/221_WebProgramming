ror: {\n        color: \"red\",\n      },\n      styleTextArea: {\n        width: \"100%\",\n        // height: this.height,\n        fontFamily: \"Poppins\",\n        padding: \"16px 32px\",\n        fontSize: \"12px\",\n        color: \"#9098b1\",\n        lineHeight: \"180%\",\n        letterSpacing: \"0.5px\",\n        fontWeight: \"bold\",\n        boxSizing: \"border-box\",\n        border: \"1px solid #ebf0ff\",\n        borderRadius: \"5px\",\n        resize: \"none\",\n      },\n      styleTextAreaDishDescription: {\n        color: \"#6a6a6a\",\n      },\n    };\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":"src/components"}]}