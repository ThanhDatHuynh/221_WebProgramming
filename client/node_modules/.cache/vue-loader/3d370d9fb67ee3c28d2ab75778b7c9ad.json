lse-if=\"text == 'Edit'\">\n    <v-btn\n      :disabled=\"isDisabled\"\n      @click=\"raiseEvent\"\n      elevation=\"2\"\n      :style=\"[style, styleEdit]\"\n      >{{ text }}</v-btn\n    >\n  </div>\n\n  <div v-else-if=\"text == 'Delete'\">\n    <v-btn\n      :disabled=\"isDisabled\"\n      @click=\"raiseEvent\"\n      elevation=\"2\"\n      :style=\"[style, styleEdit, styleDelete]\"\n      >{{ text }}</v-btn\n    >\n  </div>\n\n  <div v-else-if=\"text == 'Create Account'\">\n    <v-btn\n      :disabled=\"isDisabled\"\n      @click=\"raiseEvent\"\n      elevation=\"2\"\n      :style=\"[style, styleCreate]\"\n      >{{ text }}</v-btn\n    >\n  </div>\n\n\n  <div v-else-if=\"text == 'Add Product'\">\n    <v-btn\n      :disabled=\"isDisabled\"\n      @click=\"raiseEvent\"\n      elevation=\"2\"\n      :style=\"[style, styleAddPd]\"\n      >{{ text }}</v-btn\n    >\n  </div>\n\n  <div v-else-if=\"text == 'Add New Picture'\">\n    <v-btn\n      :disabled=\"isDisabled\"\n      @click=\"raiseEvent\"\n      elevation=\"2\"\n      :style=\"[style, styleAddPt]\"\n      >{{ text }}</v-btn\n    >\n  </div>\n\n  <div v-else-if=\"text == 'Book Your Table' || text == 'Book My Table'\">\n    <v-btn\n      :disabled=\"isDisabled\"\n      @click=\"raiseEvent\"\n      elevation=\"2\"\n      :style=\"[style, styleBook]\"\n      >{{ text }}</v-btn\n    >\n  </div>\n\n  <div v-else-if=\"text == 'Explore More'\">\n    <v-btn\n      :disabled=\"isDisabled\"\n      @click=\"raiseEvent\"\n      elevation=\"2\"\n      :style=\"[style, styleBook]\"\n      >{{ text }}</v-btn\n    >\n  </div>\n\n  <div v-else-if=\"text == 'White Button'\">\n    <v-btn\n      :disabled=\"isDisabled\"\n      @click=\"raiseEvent\"\n      elevation=\"0\"\n      :style=\"[styleWhite]\"\n      >{{ text }}</v-btn\n    >\n  </div>\n\n  <div v-else>\n    <v-btn\n      :disabled=\"isDisabled\"\n      @click=\"raiseEvent\"\n      elevation=\"2\"\n      :style=\"style\"\n      >{{ text }}</v-btn\n    >\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Button\",\n  props: {\n    text: String,\n    width: String,\n    height: String,\n    isDisabled: {\n      type: Boolean,\n      default: false,\n    },\n  },\n\n  methods: {\n    raiseEvent() {\n      this.$emit(\"onClick\");\n    },\n  },\n\n  data() {\n    return {\n      styleWhite: {\n        width: this.width,\n        height: this.height,\n        color: \"white\",\n        background: \"white\",\n        borderRadius: \"5px\",\n        fontSize: \"18px\",\n        padding: \"16px\",\n        cursor: \"auto\",\n      },\n      style: {\n        width: this.width,\n        height: this.height,\n        color: \"white\",\n        background: \"#e1651f\",\n        borderRadius: \"5px\",\n        fontSize: \"18px\",\n        padding: \"16px\",\n      },\n      styleConfirm: {\n        borderRadius: \"6px\",\n      },\n      styleCancel: {\n        color: \"#c4c4c4\",\n        background: \"transparent\",\n        border: \"1px solid #c4c4c4\",\n      },\n      styleEdit: {\n        borderRadius: \"50px\",\n        fontSize: \"14px\",\n      },\n      styleDelete: {\n        background: \"black\",\n      },\n      styleCreate: {\n        borderRadius: \"0px\",\n        fontSize: \"14px\",\n      },\n      styleAddPd: {\n        borderRadius: \"15px\",\n      },\n      styleAddPt: {\n        color: \"#e1651f\",\n        background: \"white\",\n        boxShadow: \"0px 4px 4px rgba(0, 0, 0, 0.25)\",\n      },\n      styleBook: {\n        borderRadius: \"39px\",\n        fontSize: \"14px\",\n      },\n    };\n  },\n};\n</script>\n"]}]}